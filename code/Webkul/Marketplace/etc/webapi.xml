<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/savereviewrating" method="POST">
        <service class="Webkul\Marketplace\Api\SellerReviewRatingInterface" method="saveReviewRating"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/reviews/:seller_id" method="GET">
        <service class="Webkul\Marketplace\Api\SellerReviewRatingInterface" method="getReviewRating"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>